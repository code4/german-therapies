{
  "version": 2,
  "buildCommand": "npm run build-vercel",
  "outputDirectory": "dist/public",
  "routes": [
    {
      "src": "/attached_assets/(.*)",
      "dest": "/attached_assets/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/contact.ts": {
      "runtime": "@vercel/node@3"
    }
  }
}